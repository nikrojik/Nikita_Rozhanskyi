def firstNonRepeatingLetter(s):
  dict = {}
  for c in s.lower():
    dict[c] = dict.get(c, 0) + 1
  for c in s:
    if dict[c.lower()] == 1:
      return c
  return 0

def main():
    print('Enter string:')
    string: str = input()
    x = firstNonRepeatingLetter(string)
    print(x)

if __name__ == '__main__':
    main()
