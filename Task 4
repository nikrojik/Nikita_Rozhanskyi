def countPairs(arr, target):
  count = 0
  seen = set()

  for num in arr:
    complement = target - num
    if complement in seen:
      count += 1
    seen.add(num)

  return count

def main():
    lst = [1, 3, 6, 2, 2, 0, 4, 5]
    target = 5
    print(countPairs(lst, target))

if __name__ == '__main__':
    main()
